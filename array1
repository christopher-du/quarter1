//check if array contains two of the same element(2 or 3)
public boolean double23(int[] nums) {
  int x=0;
  int y=nums.length-1;
  int xs=0;
  int zs=0;
  for (x=0;x<=y;x++){
    if (nums[x]==2)
    xs=xs+1;
    if (nums[x]==3)
    zs=zs+1;
    else
    xs=xs;
  }
  if (xs>=2 || zs>=2)
  return true;
  else
  return false;
}
